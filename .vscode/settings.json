        "aligncenter",
        "alignjustify",
        "alignleft",
        "alignright",
        "backcolor",
        "bullist",
        "chakra",
        "charmap",
        "datax",
        "formatselect",
        "Formik",
        "Humaaans",
        "insertdatetime",
        "labelledby",
        "numlist",
        "outdent",
        "removeformat",
        "searchreplace",
        "tinymce",
        "visualblocks",
        "Woohoo",
        "wordcount"
    ]
}